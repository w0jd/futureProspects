@{
    ViewData["Title"] = "Home Page";
}
<head>
  <meta charset="UTF-8">
  <title>Future Prospects</title>
  <link rel="stylesheet" type="text/css" href="~/css/contact_style.css">

</head>

<div id="main_contact">

    <div id="family_photo">
        <img src="../media/family_photo.png">
    </div>

    <section id="about">
        <div class="headline">About Project</div>
        <p>Are you a student and looking for a job? You've come to the right place! This is a website connecting employers with students looking for their dream job after graduation. <br>
          We are a group of students helping companies and other students to find employees or get a good job. We are also developing our web skills via this project.</p>
    </section>




    <section id="services">
        <h2>Our Services</h2>
        <ul>
            <li>Simple connection between university student and employer </li>
            <li>Creating job offers addressed especially to students</li>
            <li>CV-like profile creator which your employer will see</li>
        </ul>
    </section>
</div>

